{
  "name": "personal-finance-system",
  "version": "0.1.0",
  "private": true,
  "main": "app/electron.js",
  "dependencies": {
    "electron-is-dev": "^1.1.0"
  },
  "build": {
    "productName": "personal-finance-system",
    "extends": null,
    "files": [
      "build/**/*"
    ],
    "extraMetadata": {
      "main": "build/electron.js"
    },
    "directories": {
      "buildResources": "resources"
    }
  },
  "scripts": {
    "start": "concurrently \"npm run start:react\" \"npm run start:electron\"",
    "start:react": "cross-env BROWSER=none react-app-rewired start",
    "build": "npm run build:copy && npm run pack",
    "build:react": "react-app-rewired build",
    "build:copy": "npm run build:react && npm run copy:electron",
    "pack": "electron-builder",
    "copy:electron": "cp -r ./app/. ./build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "start:electron": "wait-on http://localhost:3000 && electron ."
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/history": "^4.7.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router": "^5.0.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^3.4.7",
    "antd": "^3.26.8",
    "axios": "^0.19.0",
    "bizcharts": "^3.5.5",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "customize-cra": "^0.7.0",
    "devtron": "^1.4.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "electron-debug": "^3.0.1",
    "electron-devtools-installer": "^2.2.4",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "mobx-react-router": "^4.1.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.1.2",
    "typescript": "3.6.3",
    "uuid": "^3.4.0",
    "wait-on": "^3.3.0"
  }
}
